<div class="page-header">
  <h1> <i class="fa fa-fax"></i> Booking contacts for your itinerary <small> (Sorted by country) </small></h1>
</div>


<button type="button" class="btn btn-primary btn-lg btn-block disabled">
   <span class="glyphicon glyphicon-phone-alt"> </span> Contacts
</button>

<% contacts = Contact.read %>
</br>
<table class="table table-striped table-bordered table-hover table-condensed">
  <tr>
    <th>Country</th>
    <th>Event Type</th>
    <th>Company Name</th>
    <th>Personal Contact (if any)</th>
    <th>Address</th>
    <th>Phone Number(if any)</th>
  </tr>
 <% contacts.each do |run| %>
  <tr class="active">
    <td><strong> <%= run["Name"] %> </strong></td>
    <td><%= run["Type"] %></td>
    <td><%= link_to run["Company_name"], run["Website"] %></td>
    <td><%= run["Contact"] %></td>
    <td><%= run["Address"] %></td>
    <td><%= run["Phone_number"] %></td>
  </tr>
 <% end %>
</table>
